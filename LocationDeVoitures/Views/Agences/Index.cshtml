@model IEnumerable<LocationDeVoitures.Models.Agence>
@{
    ViewBag.Title = "Index";
}


<div style="text-align: -webkit-center;">
    <div class="col-md-10 login-sec container-login">
        <h2>Liste Des Agences</h2>
        <table class="table">
            <tr>
                <th>
                    Nom
                </th>
                <th>
                    CodeAgence
                </th>
                <th>
                    Address
                </th>
                <th>
                    Ville
                </th>
                <th>
                    Tel
                </th>
                <th>
                    
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nom)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CodeAgence)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Address)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Ville)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Tel)
                    </td>
                    <td>
                        @Html.ActionLink("Details", "Details", "Agences", new { id = item.AgenceID }, null)
                    </td>
                </tr>
            }

        </table>
    </div>
</div>
